      "patch": "@@ -0,0 +1,103 @@\n+.feature.quick {\n+  .deploy-links {\n+    margin: 80px 30px;\n+    display: flex;\n+    flex-direction: column;\n+\n+    .item {\n+      cursor: pointer;\n+      display: flex;\n+      flex-direction: row;\n+      flex-wrap: wrap;\n+      place-items: flex-end;\n+      font-size: 35px;\n+      color: #000;\n+\n+      &:not(:last-child) {\n+        margin-bottom: 1.8em;\n+      }\n+\n+      &:hover {\n+        .arrow-icon {\n+          .block {\n+            width: 100%;\n+          }\n+\n+          svg {\n+            stroke: #fff;\n+          }\n+        }\n+      }\n+\n+      .bold {\n+        font-size: 1.5em;\n+        margin-right: .2em;\n+      }\n+\n+      span {\n+        margin-right: 1.4em;\n+      }\n+\n+      .arrow-icon {\n+        position: relative;\n+        display: flex;\n+        flex-direction: row;\n+        place-items: center;\n+        margin-top: 10px;\n+        height: 2em;\n+        width: calc(2em + 30px);\n+\n+        .block {\n+          left: 0;\n+          top: 0;\n+          position: absolute;\n+          display: block;\n+          background: #55ACEE;\n+          height: 2em;\n+          width: .6em;\n+          margin-right: .3em;\n+          transition: width 500ms cubic-bezier(0.075, 0.820, 0.165, 1.000);\n+        }\n+\n+        svg {\n+          padding-left: 30px;\n+          position: absolute;\n+          top: 50%;\n+          left: 50%;\n+          transform: translate(-50%, -50%);\n+        }\n+      }\n+    }\n+  }\n+\n+  .airplane-wrap {\n+    margin-top: 100px;\n+    margin: 0 80px;\n+  }\n+\n+  &.visible {\n+    @keyframes airplaneAnim {\n+      from {\n+        transform: translate(-50%, 50%);\n+      }\n+    }\n+\n+    .airplane {\n+      animation: airplaneAnim 2s cubic-bezier(0.075, 0.820, 0.165, 1.000);\n+    }\n+  }\n+\n+  .extra-desc {\n+    margin-top: 250px;\n+    margin-bottom: 250px;\n+    text-align: center;\n+\n+    .golang-icon {\n+      margin-bottom: 100px;\n+    }