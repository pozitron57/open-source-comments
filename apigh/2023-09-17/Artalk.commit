      "changes": 69,
      "blob_url": "https://github.com/ArtalkJS/Artalk/blob/da692649bc8c130dd91ad92d84b48840f8c14466/docs%2Flanding%2Fsrc%2Fcomponents%2FFeatures%2FSlightFeature.tsx",
      "raw_url": "https://github.com/ArtalkJS/Artalk/raw/da692649bc8c130dd91ad92d84b48840f8c14466/docs%2Flanding%2Fsrc%2Fcomponents%2FFeatures%2FSlightFeature.tsx",
      "contents_url": "https://api.github.com/repos/ArtalkJS/Artalk/contents/docs%2Flanding%2Fsrc%2Fcomponents%2FFeatures%2FSlightFeature.tsx?ref=da692649bc8c130dd91ad92d84b48840f8c14466",
      "patch": "@@ -0,0 +1,69 @@\n+import React, { useState } from 'react'\n+import { FeatureTitle } from '../FeatureTitle'\n+import { Reveal } from '../Reveal'\n+import { FeatureBase } from './FeatureBase'\n+import './SlightFeature.scss'\n+import { FeatureDesc } from '../FeatureDesc'\n+\n+export const SlightFeature: React.FC = () => {\n+  const [percent, setPercent] = useState(0)\n+  const [visible, setVisible] = useState(false)\n+\n+  const onVisibleChange = (visible: boolean) => {\n+    setVisible(visible)\n+    if (visible) {\n+      const fn = setInterval(() => {\n+        setPercent((prev) => {\n+          const next = Math.round(prev+0.5)\n+          if (next <= 100) {\n+            return next\n+          } else {\n+            clearInterval(fn)\n+            return 100\n+          }\n+        })\n+      }, 40)\n+    } else {\n+      setPercent(0)\n+    }\n+  }\n+\n+  return (\n+    <FeatureBase\n+      className='slight'\n+      onVisibleChange={onVisibleChange}\n+    >\n+      <FeatureTitle text='轻量' color='#77B255' />\n+\n+      <div className=\"content\">\n+        <main>\n+          <div className=\"progress-wrap\">\n+            <div className=\"progress\">\n+              <div className=\"bar\" style={{width: `${percent >= 100 || visible ? 100 : 0}%`}} />\n+            </div>\n+            <div className=\"text\">\n+              <div className=\"size\">40KB</div>\n+              <div className=\"percent\">{percent}%</div>\n+            </div>\n+          </div>\n+\n+          <Reveal delay={200}>\n+            <FeatureDesc>\n+            原生 JS 前端程序，核心功能无依赖框架，<br/>小巧体积、快速加载，迅速响应每刻交互。\n+            </FeatureDesc>\n+          </Reveal>\n+        </main>\n+\n+        <aside>\n+          <svg className='leaf-wrap' width=\"338\" height=\"330\" viewBox=\"0 0 338 330\" fill=\"none\" xmlns=\"http://www.w3.org/2000/svg\">\n+            <g className='leaf'>\n+              <path d=\"M100.05 227.714C129.808 257.472 198.146 298.863 275.608 227.298C231.821 218.875