      "patch": "@@ -0,0 +1,70 @@\n+.feature.slight {\n+  .progress-wrap {\n+    width: 100%;\n+    margin-top: 80px;\n+    margin-bottom: 50px;\n+\n+    .progress {\n+      margin-bottom: 15px;\n+      position: relative;\n+      height: 24px;\n+      width: 100%;\n+      background: #D9D9D9;\n+      border-radius: 80px;\n+      overflow: hidden;\n+\n+      .bar {\n+        position: absolute;\n+        height: 100%;\n+        width: 0%;\n+        background: #77B255;\n+        transition: all 800ms cubic-bezier(0.230, 1.000, 0.320, 1.000);\n+      }\n+    }\n+\n+    .text {\n+      display: flex;\n+      flex-direction: row;\n+      justify-content: space-between;\n+      place-items: center;\n+      padding: 0 5px;\n+      font-size: 23px;\n+\n+      .size {\n+        color: #77B255;\n+        font-weight: bold;\n+      }\n+\n+      .percent {\n+        color: #67757F;\n+      }\n+    }\n+  }\n+\n+  &.visible {\n+    .leaf-wrap {\n+      @keyframes windAnim {\n+        from {\n+          opacity: 0;\n+          filter: blur(10px);\n+          transform: translate(-40%, 35%) rotate(20deg);\n+        }\n+        to {\n+        }\n+      }\n+      .wind {\n+        animation: windAnim .5s cubic-bezier(0.230, 1.000, 0.320, 1.000);\n+      }\n+\n+      @keyframes leafAnim {\n+        0% {\n+          transform: translate(-10%, -10%) rotate(10deg);\n+        }\n+      }\n+\n+      .leaf {\n+        animation: leafAnim 1s cubic-bezier(0.250, 0.460, 0.450, 0.940);\n+      }\n+    }\n+  }\n+}"
