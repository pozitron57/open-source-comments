      "patch": "@@ -0,0 +1,23 @@\n+.reveal {\n+  @keyframes fadeInUp {\n+    0% {\n+      opacity: 0;\n+      transform: translate3d(0,100%,0);\n+    }\n+\n+    100% {\n+      opacity: 1;\n+      transform: translateZ(0);\n+    }\n+  }\n+  opacity: 0;\n+\n+  &.animate {\n+    animation-name: fadeInUp;\n+    animation-iteration-count: 1;\n+  }\n+\n+  &.show {\n+    opacity: 1;\n+  }\n+}"
