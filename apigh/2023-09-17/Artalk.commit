      "raw_url": "https://github.com/ArtalkJS/Artalk/raw/da692649bc8c130dd91ad92d84b48840f8c14466/docs%2Flanding%2Fsrc%2Fhooks%2Fvisible.ts",
      "contents_url": "https://api.github.com/repos/ArtalkJS/Artalk/contents/docs%2Flanding%2Fsrc%2Fhooks%2Fvisible.ts?ref=da692649bc8c130dd91ad92d84b48840f8c14466",
      "patch": "@@ -0,0 +1,21 @@\n+import { useEffect, useState } from 'react'\n+\n+export const useIsElementVisible = (target: React.RefObject<HTMLDivElement>) => {\n+  const [isVisible, setIsVisible] = useState(false)\n+\n+  useEffect(() => {\n+    const observer = new IntersectionObserver((entries) => {\n+      setIsVisible(entries[0].isIntersecting)\n+    }, {\n+      threshold: 0.4\n+    })\n+\n+    target.current && observer.observe(target.current)\n+\n+    return () => {\n+      target.current && observer.unobserve(target.current)\n+    }\n+  }, [target])\n+\n+  return isVisible\n+}"
    },
