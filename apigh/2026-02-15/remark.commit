      "patch": "@@ -0,0 +1,139 @@\n+<lexer>\n+  <config>\n+    <name>microcad</name>\n+    <alias>cad</alias>\n+    <filename>*.cad</filename>\n+    <filename>*.ucad</filename>\n+    <filename>*.mcad</filename>\n+    <mime_type>text/microcad</mime_type>\n+    <ensure_nl>true</ensure_nl>\n+  </config>\n+\n+  <rules>\n+    <!-- Root state -->\n+    <state name=\"root\">\n+      <rule pattern=\"\\n\">\n+        <token type=\"TextWhitespace\"/>\n+      </rule>\n+      <rule pattern=\"\\s+\">\n+        <token type=\"TextWhitespace\"/>\n+      </rule>\n+\n+      <!-- COMMENTS -->\n+      <rule pattern=\"///.*\">\n+        <token type=\"LiteralStringDoc\"/>\n+      </rule>\n+\n+      <rule pattern=\"//.*\">\n+        <token type=\"CommentSingle\"/>\n+      </rule>\n+\n+      <rule pattern=\"/\\*.*\">\n+        <token type=\"CommentMultiline\"/>\n+        <push state=\"comment\"/>\n+      </rule>\n+\n+      <!-- KEYWORDS -->\n+      <rule pattern=\"\\b(pub|sketch|part|op|mod|use|fn|const|prop|init|return|if|else|mat|__builtin|or|and|not|as)\\b\">\n+        <token type=\"Keyword\"/>\n+      </rule>\n+\n+      <!-- TYPES -->\n+      <rule pattern=\"\\b(Integer|Scalar|String|Color|Length|Area|Volume|Angle|Weight|Density|Bool|Matrix[0-9])\\b\">\n+        <token type=\"KeywordType\"/>\n+      </rule>\n+\n+      <!-- FUNCTION NAMES -->\n+      <rule pattern=\"\\b([a-z_][a-zA-Z0-9_]*)\\s*(?=\\()\">\n+        <token type=\"NameFunction\"/>\n+      </rule>\n+\n+      <!-- OPERATORS -->\n+      <rule pattern=\"[+\\-*/%&amp;|&lt;&gt;^!@=]\">\n+        <token type=\"Operator\"/>\n+      </rule>\n+\n+      <!-- NUMBERS + UNITS -->\n+      <rule pattern=\"\\b(([0-9]+(\\.[0-9]+)?)(%|m|cm|mm|m|in|ft|yd|m|cm|mm|m|in|ft|yd|ml|cl|l|l|cm|mm|m|m|in|ft|yd|deg||grad|turn|rad|g|kg|lb|oz)?)|true|false\\b\">\n+        <token type=\"LiteralNumber\"/>\n+      </rule>\n+\n+      <!-- NAMESPACES -->\n+      <rule pattern=\"([a-z_][a-z0-9_]*)(::)\">\n+        <bygroups>\n+          <token type=\"NameNamespace\"/>\n+          <token type=\"Operator\"/>\n+        </bygroups>\n+      </rule>\n+\n+      <!-- CONSTANTS (ALL CAPS) -->\n+      <rule pattern=\"\\b([A-Z_][A-Z0-9_]*)\\b\">\n+        <token type=\"NameConstant\"/>\n+      </rule>\n+\n+      <!-- CUSTOM TYPES (Capitalized identifiers) -->\n+      <rule pattern=\"\\b([A-Z_][a-zA-Z0-9_]*)\\b\">\n+        <token type=\"NameClass\"/>\n+      </rule>\n+\n+      <!-- VARIABLES -->\n+      <rule pattern=\"\\b([a-z_][a-zA-Z0-9_]*)\\b\">\n+        <token type=\"Name\"/>\n+      </rule>\n+\n+      <!-- STRINGS -->\n+      <rule pattern=\"&quot;[^&quot;]*&quot;\">\n+        <token type=\"LiteralString\"/>\n+      </rule>\n+\n+      <!-- PAREN GROUP -->\n+      <rule pattern=\"[{}()\\[\\],.;:]\">\n+        <token type=\"Punctuation\"/>\n+      </rule>\n+\n+\n+      <!-- BRACE GROUP -->\n+      <rule pattern=\"\\{\">\n+        <token type=\"Punctuation\"/>\n+        <push state=\"brace\"/>\n+      </rule>\n+\n+      <rule pattern=\"[\\}\\)]\">\n+        <token type=\"Punctuation\"/>\n+      </rule>\n+\n+    </state>\n+\n+    <!-- Parenthesized expression -->\n+    <state name=\"paren\">\n+      <rule pattern=\"\\)\">\n+        <token type=\"Punctuation\"/>\n+        <pop/>\n+      </rule>\n+      <rule>\n+        <include state=\"root\"/>\n+      </rule>\n+    </state>\n+\n+    <!-- Braced expression -->\n+    <state name=\"brace\">\n+      <rule pattern=\"\\}\">\n+        <token type=\"Punctuation\"/>\n+        <pop/>\n+      </rule>\n+      <rule>\n+        <include state=\"root\"/>\n+      </rule>\n+    </state>\n+\n+    <state name=\"comment\">\n+      <!-- End of comment -->\n+      <rule pattern=\"\\*/\">\n+        <token type=\"CommentMultiline\"/>\n+        <pop/>\n+      </rule>\n+    </state>\n+\n+\n+  </rules>\n+</lexer>"
    },
